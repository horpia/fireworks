<label>Color:</label>
<fw-color-picker name="color" autofill></fw-color-picker>

<label>Lights:</label>
<fw-input-range name="elements" min="{{elementsMin}}" max="{{elementsMax}}"
					   value="{{elements}}"></fw-input-range>
<label>Distance:</label>
<fw-input-range name="distance" min="{{distanceMin}}" max="{{distanceMax}}"
					   value="{{distance}}"></fw-input-range>